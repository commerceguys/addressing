{
	"locale": "und",
	"format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%postalCode %locality",
	"required_fields": [
		"recipient",
		"addressLine1",
		"locality",
		"postalCode"
	],
	"uppercase_fields": [
